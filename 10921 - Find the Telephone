#include <iostream>
#include <string>
#include<stdio.h>

using namespace std;

int main()
{
    char ch;
    int i=0;
    while(scanf("%c",&ch)!=EOF)
    {
        if(ch=='A'||ch=='B'||ch=='C')
        {
            printf("2");
        }
        else if(ch=='E'||ch=='F'||ch=='D')
        {
            printf("3");
        }
         else if(ch=='H'||ch=='I'||ch=='G')
        {
            printf("4");
        }
         else if(ch=='J'||ch=='K'||ch=='L')
        {
            printf("5");
        }
         else if(ch=='M'||ch=='N'||ch=='O')
        {
            printf("6");
        }
         else if(ch=='P'||ch=='Q'||ch=='R' || ch=='S')
        {
            printf("7");
        }
         else if(ch=='T'||ch=='U'||ch=='V')
        {
            printf("8");
        }
         else if(ch=='W'||ch=='X'||ch=='Y' || ch=='Z')
        {
            printf("9");
        }
        else printf("%c",ch);
    }
    return 0;
}
