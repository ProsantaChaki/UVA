#include<iostream>
using namespace std;

int main()
{
    int t,u,v;
    cin>>t;
    while(t!=0)
    {
       cin>>u;
       cin>>v;
       if(u>v)
       {
           cout<<">"<<endl;
       }
       else if(u<v)
       {
           cout<<"<"<<endl;
       }
       else
       cout<<"="<<endl;

       t--;

    }
    return 0;

}
